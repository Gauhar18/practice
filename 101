#include <iostream>
#include <string>
using namespace std;
int main()
{
 for(int i = 10102; i <= 99999; ++i)
    if((i / 100) % 2 == 1)
        if((i % 10 + (i / 10) % 10 + (i / 100) % 10 + (i / 1000) % 10 + (i / 10000)) % 4 == 0) //Тут можно не костылять, если есть ToString
            cout << i << " ";
cout << endl;
return 0;
}
