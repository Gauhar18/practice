#include <iostream>
#include <cmath>
 using namespace std;
int main(){
 
    unsigned n;
    cout << "n = "; 
    cin >> n;
    double x;
    cout << "x = "; 
    cin >> x;
 
    double sum = 0;
    for ( unsigned u = 1; u <= n; ++u )
        sum += cos( u*x )/u;
 
    cout << sum;
 
    return 0;
}
