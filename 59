#include <iostream>

#include <math.h>

using namespace std;
int main()
{ 

int n;
int m4;
int m3;
int m2; 
int m1;
cout << "enter a four digit number" ;
cin >> n;
{
 
if (n <1000) cout << "not four digit "; 
}
m4 = n % 10;
m3 = n  / 10 % 10;
m2 = n / 10 / 10 % 10;
m1 = n / 10 / 10 / 10 % 10;
cout<< m1 <<" " << m2<< " " << m3<< " " << m4 << " " ;
cout << "\n";
 
if (m1>m2>m3>m4) {
    cout <<"yes"<<endl;
}
else {
 cout << "no" <<endl;
}
 
     return 0;
}
