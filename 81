#include <iostream>
using namespace std;
int main(){
for (int i = 10; i <= 100; cout << i << ' ', i += 10);
cout << endl;
for (int i = 2; i <= 100; ++i)
if (i % 2 == 0 && i % 5 == 0)
cout << i << ' ';
cout << endl;
int i = 10;
do{
cout << i << ' ';
i += 10;
}while(i <= 100);
cout << endl;
i = 2;
do{
if (i % 2 == 0 && i % 5 == 0)
cout << i << ' ';
++i;
}while(i <= 100);
cout << endl;
cin.get();
return 0;
}
